<%= render 'teams/nav_tabs', team: @team do %>
  <%= render 'teams/exercises/exercises' %>
<% end %>

<script text="text/javascript">
  function createModal(modalName) {
    var div = $(document.createElement('div'))
    div.attr('id', modalName + '-container');
    $('body').append(div);
    $(div).html('<%= j(render "teams/exercises/exercises_modal") %>');
    $('body').append(div);
  }

  createModal('exercises')
</script>
